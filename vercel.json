{
    "version": 2,
    "builds": [
      {
        "src": "src/index.server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
    {
      "src": "/api-docs/(.*)",
      "dest": "src/index.server.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.server.js"
    }
  ]
  }